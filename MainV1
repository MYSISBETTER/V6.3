TimeLabel.TextColor3 = Color3.fromRGB(220, 220, 220)
TimeLabel.TextSize = 14
TimeLabel.TextXAlignment = Enum.TextXAlignment.Left
TimeLabel.Text = "Time: " .. os.date("%I:%M %p")
TimeLabel.Parent = BottomBar

